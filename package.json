{
  "name": "bookkeeper",
  "version": "0.3.3",
  "main": "src/server/BookkeeperServer.ts",
  "private": true,
  "scripts": {
    "postinstall": "yarn update-revisions",
    "create-dev-db": "docker run -e POSTGRES_PASSWORD=postgres -p 15488:5432 --name bookkeeper-db -d postgres:11.8",
    "watch-client": "yarn update-revision client && react-scripts start",
    "watch-server": "yarn update-revision server && yarn migrate && nodemon",
    "test-client": "react-scripts test",
    "test": "yarn test-client",
    "build-client": "react-scripts build",
    "build-server": "tsc -p ./tsconfig.prod.json",
    "build-all": "yarn update-revisions && yarn build-server && yarn build-client",
    "start-server-prod": "DEBUG=bookkeeper* NODE_ENV=production node build-server/server/BookkeeperServer.js",
    "run-server": "cross-env NODE_ENV=production node -r ./tsconfig-paths-bootstrap.js build-server/server/BookkeeperServer.js",
    "eject-client": "react-scripts eject",
    "update-revision": "bash script/update-revision.sh",
    "update-revisions": "yarn update-revision server && yarn update-revision client",
    "lint": "eslint \"src/**/*.ts\" \"src/**/*.tsx\"",
    "deploy": "sh script/deploy.sh",
    "clean": "rm -rf build/ dist/ build-server/",
    "ps-server": "ps -ef | grep ./src/server/BokkeeperServer.ts | grep -v grep | awk '{ print $2; }'",
    "kill-server": "ps -ef | grep ./src/server/BookkeeperServer.ts | grep -v grep | awk '{ print $2; }' | xargs kill",
    "migrate": "knex migrate:latest",
    "migrate-make": "knex migrate:make",
    "migrate-rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "example-data": "DEBUG=bookkeeper*,net* ts-node --files --project tsconfig.server.json src/tools/addExampleData.ts"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:thaapasa/bookkeeper.git"
  },
  "author": "Anu & Tuukka Haapasalo",
  "license": "ISC",
  "homepage": "https://kukkaro.pomeranssi.fi",
  "dependencies": {
    "@date-io/core": "1.3.13",
    "@date-io/moment": "1.3.13",
    "@material-ui/core": "4.11.0",
    "@material-ui/icons": "4.9.1",
    "@material-ui/pickers": "3.2.10",
    "assert": "^2.0.0",
    "baconjs": "3.0.17",
    "big.js": "6.2.1",
    "body-parser": "1.20.0",
    "d3": "5.16.0",
    "d3-axis": "1.0.12",
    "d3-interpolate": "1.4.0",
    "d3-scale": "3.2.1",
    "d3-selection": "1.4.1",
    "debug": "4.3.4",
    "dotenv": "16.0.1",
    "express": "4.18.1",
    "fp-ts": "2.12.1",
    "history": "5.3.0",
    "io-ts": "2.2.16",
    "moment": "2.29.4",
    "node-fetch": "2.6.7",
    "path": "0.12.7",
    "pg": "8.7.3",
    "pg-promise": "10.11.1",
    "react": "18.2.0",
    "react-autosuggest": "10.1.0",
    "react-dom": "^18.2.0",
    "react-router": "5.3.3",
    "react-router-dom": "5.3.3",
    "styled-components": "5.3.5",
    "tsconfig-paths": "4.0.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/big.js": "6.1.5",
    "@types/body-parser": "1.19.2",
    "@types/d3": "5.7.2",
    "@types/d3-axis": "1.0.12",
    "@types/debug": "4.1.7",
    "@types/express": "4.17.13",
    "@types/jest": "28.1.6",
    "@types/node": "14.14.20",
    "@types/node-fetch": "2.6.2",
    "@types/react": "18.0.15",
    "@types/react-autosuggest": "10.1.5",
    "@types/react-dom": "18.0.6",
    "@types/react-router": "5.1.18",
    "@types/react-router-dom": "5.3.3",
    "@types/styled-components": "5.1.25",
    "@typescript-eslint/eslint-plugin": "5.30.6",
    "@typescript-eslint/parser": "5.30.6",
    "cross-env": "7.0.3",
    "eslint": "8.20.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.30.1",
    "knex": "0.21.2",
    "nodemon": "2.0.19",
    "prettier": "2.7.1",
    "react-scripts": "5.0.1",
    "ts-node": "10.9.1",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  },
  "directories": {
    "test": "test"
  },
  "proxy": "http://localhost:3100",
  "description": "Kukkaro - a web tool for tracking your income and expenses.",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "yarn@3.2.1"
}
