  "homepage": "https://kukkaro.pomeranssi.fi",
  "dependencies": {
    "baconjs": "1.0.1",
    "big.js": "5.0.3",
    "bluebird": "3.5.1",
    "body-parser": "1.18.2",
    "dotenv": "4.0.0",
    "express": "4.16.2",
    "material-ui": "0.20.0",
    "moment": "2.20.1",
    "pg": "7.4.1",
    "pg-pool": "2.0.3",
    "prop-types": "15.6.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
