{
  "name": "bookkeeper",
  "version": "0.7.1",
  "main": "src/server/BookkeeperServer.ts",
  "private": true,
  "scripts": {
    "postinstall": "bun run update-revisions",
    "create-dev-db": "docker run -e POSTGRES_PASSWORD=postgres -p 15488:5432 --name bookkeeper-db -d postgres:14.3",
    "watch-client": "bun run update-revision client && react-scripts start",
    "watch-server": "bun run update-revision server && bun run migrate && bun run --watch src/server/BookkeeperServer.ts",
    "test-client": "react-scripts test --runInBand",
    "test": "bun run test-client",
    "build-client": "react-scripts build",
    "build-server": "tsc -p ./tsconfig.prod.json",
    "build-all": "bun run update-revisions && bun run build-server && bun run build-client",
    "start-server-prod": "DEBUG=bookkeeper* NODE_ENV=production node build-server/server/BookkeeperServer.ts",
    "run-server": "NODE_ENV=production bun run src/server/BookkeeperServer.ts",
    "eject-client": "react-scripts eject",
    "update-revision": "bash script/update-revision.sh",
    "update-revisions": "bun run update-revision server && bun run update-revision client",
    "lint": "biome check src",
    "format": "biome format src --write",
    "lint-biome": "biome format src --write",
    "lint-tsc": "tsc --noEmit",
    "deploy": "sh script/deploy.sh",
    "clean": "rm -rf build/ dist/ build-server/",
    "ps-server": "ps -ef | grep ./src/server/BokkeeperServer.ts | grep -v grep | awk '{ print $2; }'",
    "kill-server": "ps -ef | grep ./src/server/BookkeeperServer.ts | grep -v grep | awk '{ print $2; }' | xargs kill",
    "migrate": "knex migrate:latest",
    "migrate-make": "knex migrate:make",
    "migrate-rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "example-data": "SWC_NODE_PROJECT=tsconfig.server.json DEBUG=\"bookkeeper*,net*\" node -r dotenv/config -r tsconfig-paths/register -r @swc-node/register src/tools/addExampleData.ts"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:thaapasa/bookkeeper.git"
  },
  "author": "Anu & Tuukka Haapasalo",
  "license": "ISC",
  "homepage": "https://kukkaro.pomeranssi.fi",
  "dependencies": {
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@mui/icons-material": "5.14.8",
    "@mui/material": "5.14.8",
    "@mui/system": "5.14.8",
    "@mui/x-date-pickers": "6.13.0",
    "@types/node-fetch": "2.6.4",
    "assert": "2.1.0",
    "baconjs": "3.0.17",
    "big.js": "6.2.1",
    "body-parser": "1.20.2",
    "debug": "4.3.4",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "history": "5.3.0",
    "moment": "2.29.4",
    "node-fetch": "2.7.0",
    "path": "0.12.7",
    "pg": "8.11.3",
    "pg-promise": "11.5.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router": "6.15.0",
    "react-router-dom": "6.15.0",
    "recharts": "2.8.0",
    "zod": "3.22.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "7.21.11",
    "@biomejs/biome": "1.1.2",
    "@types/big.js": "6.2.0",
    "@types/body-parser": "1.19.2",
    "@types/debug": "4.1.8",
    "@types/express": "4.17.17",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@types/react-router": "5.1.20",
    "@types/react-router-dom": "5.3.3",
    "bun-types": "1.0.1",
    "knex": "2.5.1",
    "react-scripts": "5.0.1",
    "tslib": "2.6.2",
    "typescript": "5.2.2"
  },
  "resolutions": {
    "nth-check": "2.1.1",
    "react": "18.2.0"
  },
  "proxy": "http://localhost:3100",
  "description": "Kukkaro - a web tool for tracking your income and expenses.",
  "browserslist": {
    "production": [">0.2%", "not dead", "not op_mini all"],
    "development": ["last 1 chrome version", "last 1 firefox version", "last 1 safari version"]
  }
}
