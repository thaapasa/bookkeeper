{
  "name": "bookkeeper",
  "version": "0.1.1",
  "main": "bookkeeper-server.js",
  "private": true,
  "scripts": {
    "start": "npm run update:revision && npm run start:server",
    "update:revision": "script/update-revision.sh",
    "start:client": "react-scripts-ts start",
    "build:client": "react-scripts-ts build",
    "test:client": "react-scripts-ts test --env=jsdom",
    "eject:client": "react-scripts-ts eject",
    "start:server": "NODE_ENV=development node src/server/bookkeeper-server.js",
    "test": "npm run build:test && npm run run:test",
    "test:server": "node_modules/mocha/bin/mocha src/test/server/api/*.js",
    "build:prod": "NODE_ENV=production node_modules/browserify/bin/cmd.js src/client/client.js -t [ envify --NODE_ENV production ] -t [ babelify --presets [ es2015 react ] ] -o public/js/bookkeeper.js",
    "build:test": "NODE_ENV=development node_modules/browserify/bin/cmd.js --debug src/test/shared/**/*.js -t [ envify --NODE_ENV development ] -t [ babelify --presets [ es2015 react ] ] > test/test.js",
    "build": "npm run build:client",
    "prod": "npm run build:prod && script/create-dist.sh"
  },
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:haapasalot/bookkeeper.git"
  },
  "author": "Anu & Tuukka Haapasalo",
  "license": "ISC",
  "homepage": "https://bitbucket.org/haapasalot/bookkeeper#readme",
  "dependencies": {
    "baconjs": "^0.7.89",
    "big.js": "^3.1.3",
    "bluebird": "^3.4.6",
    "body-parser": "^1.15.2",
    "crypto": "0.0.3",
    "express": "^4.14.0",
    "material-ui": "^0.18.1",
    "moment": "^2.17.1",
    "pg": "^6.1.0",
    "pg-pool": "^1.6.0",
    "prop-types": "^15.5.10",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-router-dom": "^4.0.0",
    "react-tap-event-plugin": "^2.0.1",
    "superagent": "^3.0.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "chai": "^3.5.0",
    "envify": "^3.4.1",
    "mocha": "^3.2.0",
    "react-scripts-ts": "2.8.0",
    "watchify": "^3.7.0"
  },
  "directories": {
    "test": "test"
  },
  "description": "Kukkaro - a web tool for tracking your income and expenses."
}
