{
  "name": "bookkeeper",
  "version": "0.1.1",
  "main": "src/server/BookkeeperServer.ts",
  "private": true,
  "scripts": {
    "postinstall": "npm run update-revision",
    "watch-client": "react-scripts-ts start",
    "watch-server": "npm run update-revision && nodemon",
    "test-client": "react-scripts-ts test --env=jsdom",
    "test": "npm run test-client",
    "build-client": "react-scripts-ts build",
    "build-server": "tsc",
    "build-all": "npm run update-revision && npm run build-server && npm run build-client",
    "start-server-prod": "DEBUG=bookkeeper* NODE_ENV=production node build-server/server/BookkeeperServer.js",
    "eject-client": "react-scripts-ts eject",
    "update-revision": "sh script/update-revision.sh",
    "clean": "rm -rf build/ dist/ build-server/",
    "ps-server": "ps -ef | grep ./src/server/BokkeeperServer.ts | grep -v grep | awk '{ print $2; }'",
    "kill-server": "ps -ef | grep ./src/server/BookkeeperServer.ts | grep -v grep | awk '{ print $2; }' | xargs kill"
  },
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:haapasalot/bookkeeper.git"
  },
  "author": "Anu & Tuukka Haapasalo",
  "license": "ISC",
  "homepage": "https://kukkaro.pomeranssi.fi",
  "dependencies": {
    "baconjs": "2.0.4",
    "big.js": "5.0.3",
    "body-parser": "1.18.2",
    "d3": "4.13.0",
    "d3-axis": "1.0.8",
    "d3-interpolate": "1.1.6",
    "d3-scale": "2.0.0",
    "d3-selection": "1.3.0",
    "debug": "3.1.0",
    "dotenv": "5.0.0",
    "express": "4.16.2",
    "material-ui": "0.20.0",
    "moment": "2.20.1",
    "node-fetch": "2.0.0",
    "pg": "7.4.1",
    "pg-pool": "2.0.3",
    "prop-types": "15.6.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-router-dom": "4.2.2",
    "react-tap-event-plugin": "3.0.2",
    "styled-components": "3.1.6"
  },
  "devDependencies": {
    "@types/baconjs": "0.7.33",
    "@types/big.js": "4.0.2",
    "@types/body-parser": "1.16.8",
    "@types/d3": "4.12.0",
    "@types/d3-axis": "1.0.9",
    "@types/dotenv": "4.0.2",
    "@types/express": "4.11.1",
    "@types/jest": "22.1.2",
    "@types/material-ui": "0.20.5",
    "@types/node-fetch": "1.6.7",
    "@types/pg-pool": "0.0.3",
    "@types/react": "16.0.36",
    "@types/react-dom": "16.0.3",
    "@types/react-router": "4.0.21",
    "@types/react-router-dom": "4.2.3",
    "@types/react-tap-event-plugin": "0.0.30",
    "react-scripts-ts": "2.13.0",
    "tslib": "1.9.0",
    "tslint-react": "3.5.0",
    "typescript": "2.7.1"
  },
  "directories": {
    "test": "test"
  },
  "proxy": "http://localhost:3100",
  "description": "Kukkaro - a web tool for tracking your income and expenses."
}
