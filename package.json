{
  "name": "bookkeeper",
  "version": "0.2.0",
  "main": "src/server/BookkeeperServer.ts",
  "private": true,
  "scripts": {
    "postinstall": "yarn update-revision",
    "create-dev-db": "docker run -p 15488:5432 --name bookkeeper-db -d postgres:9.6.15",
    "watch-client": "react-scripts start",
    "watch-server": "yarn update-revision && yarn migrate && nodemon",
    "test-client": "jest --runInBand",
    "test": "yarn test-client",
    "build-client": "react-scripts build",
    "build-server": "tsc",
    "build-all": "yarn update-revision && yarn build-server && yarn build-client",
    "start-server-prod": "DEBUG=bookkeeper* NODE_ENV=production node build-server/server/BookkeeperServer.js",
    "eject-client": "react-scripts eject",
    "update-revision": "sh script/update-revision.sh",
    "lint": "tslint --project .",
    "deploy": "sh script/deploy.sh",
    "clean": "rm -rf build/ dist/ build-server/",
    "ps-server": "ps -ef | grep ./src/server/BokkeeperServer.ts | grep -v grep | awk '{ print $2; }'",
    "kill-server": "ps -ef | grep ./src/server/BookkeeperServer.ts | grep -v grep | awk '{ print $2; }' | xargs kill",
    "migrate": "knex migrate:latest",
    "migrate-make": "knex migrate:make",
    "migrate-rollback": "knex migrate:rollback",
    "seed": "knex seed:run"
  },
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:haapasalot/bookkeeper.git"
  },
  "author": "Anu & Tuukka Haapasalo",
  "license": "ISC",
  "homepage": "https://kukkaro.pomeranssi.fi",
  "dependencies": {
    "baconjs": "2.0.10",
    "big.js": "5.2.2",
    "body-parser": "1.18.3",
    "d3": "5.7.0",
    "d3-axis": "1.0.12",
    "d3-interpolate": "1.3.2",
    "d3-scale": "2.1.2",
    "d3-selection": "1.3.2",
    "debug": "4.1.1",
    "dotenv": "8.1.0",
    "express": "4.16.4",
    "history": "4.7.2",
    "material-ui": "0.20.2",
    "moment": "2.23.0",
    "node-fetch": "2.3.0",
    "path": "0.12.7",
    "pg": "7.8.0",
    "pg-promise": "8.5.4",
    "react": "16.10.2",
    "react-dom": "16.10.2",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "styled-components": "4.1.3"
  },
  "devDependencies": {
    "@types/baconjs": "0.7.33",
    "@types/big.js": "4.0.5",
    "@types/body-parser": "1.17.0",
    "@types/d3": "5.5.0",
    "@types/d3-axis": "1.0.11",
    "@types/dotenv": "6.1.0",
    "@types/express": "4.16.0",
    "@types/jest": "24.0.18",
    "@types/material-ui": "0.21.5",
    "@types/node": "12.7.12",
    "@types/node-fetch": "2.1.4",
    "@types/react": "16.9.5",
    "@types/react-dom": "16.9.1",
    "@types/react-router": "4.4.3",
    "@types/react-router-dom": "4.3.1",
    "@types/styled-components": "4.1.5",
    "knex": "0.19.5",
    "nodemon": "1.19.3",
    "react-scripts": "3.2.0",
    "ts-node": "8.3.0",
    "tslib": "1.10.0",
    "typescript": "3.6.4"
  },
  "directories": {
    "test": "test"
  },
  "proxy": "http://localhost:3100",
  "description": "Kukkaro - a web tool for tracking your income and expenses.",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
