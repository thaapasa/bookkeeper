{
  "name": "bookkeeper",
  "version": "0.3.0",
  "main": "src/server/BookkeeperServer.ts",
  "private": true,
  "scripts": {
    "postinstall": "yarn update-revision",
    "create-dev-db": "docker run -p 15488:5432 --name bookkeeper-db -d postgres:9.6.15",
    "watch-client": "react-scripts start",
    "watch-server": "yarn update-revision && yarn migrate && nodemon",
    "test-client": "react-scripts test",
    "test": "yarn test-client",
    "build-client": "react-scripts build",
    "build-server": "yarn tsc -p ./tsconfig.prod.json",
    "build-all": "yarn update-revision && yarn build-server && yarn build-client",
    "start-server-prod": "DEBUG=bookkeeper* NODE_ENV=production node build-server/server/BookkeeperServer.js",
    "run-server": "NODE_ENV=production node build-server/server/BookkeeperServer.js",
    "eject-client": "react-scripts eject",
    "update-revision": "sh script/update-revision.sh",
    "lint": "eslint \"src/**/*.ts\" \"src/**/*.tsx\"",
    "deploy": "sh script/deploy.sh",
    "clean": "rm -rf build/ dist/ build-server/",
    "ps-server": "ps -ef | grep ./src/server/BokkeeperServer.ts | grep -v grep | awk '{ print $2; }'",
    "kill-server": "ps -ef | grep ./src/server/BookkeeperServer.ts | grep -v grep | awk '{ print $2; }' | xargs kill",
    "migrate": "knex migrate:latest",
    "migrate-make": "knex migrate:make",
    "migrate-rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "example-data": "DEBUG=bookkeeper*,net* ts-node --files --project tsconfig.server.json src/tools/addExampleData.ts"
  },
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:haapasalot/bookkeeper.git"
  },
  "author": "Anu & Tuukka Haapasalo",
  "license": "ISC",
  "homepage": "https://kukkaro.pomeranssi.fi",
  "dependencies": {
    "@date-io/core": "1.3.11",
    "@date-io/moment": "1.3.11",
    "@material-ui/core": "4.5.1",
    "@material-ui/icons": "4.5.1",
    "@material-ui/pickers": "3.2.7",
    "baconjs": "3.0.10",
    "big.js": "5.2.2",
    "body-parser": "1.19.0",
    "d3": "5.12.0",
    "d3-axis": "1.0.12",
    "d3-interpolate": "1.3.2",
    "d3-scale": "3.1.0",
    "d3-selection": "1.4.0",
    "debug": "4.1.1",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "fp-ts": "2.1.0",
    "history": "4.10.1",
    "io-ts": "2.0.1",
    "moment": "2.24.0",
    "node-fetch": "2.6.0",
    "path": "0.12.7",
    "pg": "7.12.1",
    "pg-promise": "9.3.3",
    "react": "16.10.2",
    "react-autosuggest": "9.4.3",
    "react-dom": "16.10.2",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "styled-components": "4.4.0"
  },
  "devDependencies": {
    "@types/big.js": "4.0.5",
    "@types/body-parser": "1.17.1",
    "@types/d3": "5.7.2",
    "@types/d3-axis": "1.0.12",
    "@types/debug": "4.1.5",
    "@types/dotenv": "6.1.1",
    "@types/express": "4.17.1",
    "@types/jest": "24.0.19",
    "@types/node": "12.11.1",
    "@types/node-fetch": "2.5.2",
    "@types/react": "16.9.9",
    "@types/react-autosuggest": "9.3.12",
    "@types/react-dom": "16.9.1",
    "@types/react-router": "5.1.2",
    "@types/react-router-dom": "5.1.0",
    "@types/styled-components": "4.1.19",
    "@typescript-eslint/eslint-plugin": "2.4.0",
    "@typescript-eslint/parser": "2.4.0",
    "eslint": "6.5.1",
    "eslint-config-prettier": "6.4.0",
    "eslint-plugin-prettier": "3.1.1",
    "knex": "0.20.0",
    "nodemon": "1.19.4",
    "prettier": "1.18.2",
    "react-scripts": "3.2.0",
    "ts-node": "8.3.0",
    "tslib": "1.10.0",
    "typescript": "3.6.4"
  },
  "directories": {
    "test": "test"
  },
  "proxy": "http://localhost:3100",
  "description": "Kukkaro - a web tool for tracking your income and expenses.",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
