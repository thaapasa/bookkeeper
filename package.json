{
  "name": "bookkeeper",
  "version": "0.1.1",
  "main": "src/server/bookkeeper-server.ts",
  "private": true,
  "scripts": {
    "postinstall": "npm run update-revision",
    "watch-client": "react-scripts-ts start",
    "watch-server": "npm run update-revision && nodemon",
    "test-client": "react-scripts-ts test --env=jsdom",
    "test": "npm run test-client",
    "build-client": "react-scripts-ts build",
    "build-server": "tsc",
    "build-all": "npm run update-revision && npm run build-server && npm run build-client",
    "start-server-prod": "DEBUG=bookkeeper* NODE_ENV=production node build-server/server/bookkeeper-server.js",
    "eject-client": "react-scripts-ts eject",
    "update-revision": "script/update-revision.sh",
    "clean": "rm -rf build/ dist/",
    "ps-server": "ps -ef | grep ./src/server/bookkeeper-server.ts | grep -v grep | awk '{ print $2; }'",
    "kill-server": "ps -ef | grep ./src/server/bookkeeper-server.ts | grep -v grep | awk '{ print $2; }' | xargs kill"
  },
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:haapasalot/bookkeeper.git"
  },
  "author": "Anu & Tuukka Haapasalo",
  "license": "ISC",
  "homepage": "https://kukkaro.pomeranssi.fi",
  "dependencies": {
    "baconjs": "1.0.1",
    "big.js": "5.0.3",
    "body-parser": "1.18.2",
    "d3": "4.12.2",
    "d3-axis": "1.0.8",
    "d3-interpolate": "1.1.6",
    "d3-scale": "1.0.7",
    "d3-selection": "1.2.0",
    "debug": "3.1.0",
    "dotenv": "4.0.0",
    "express": "4.16.2",
    "material-ui": "0.20.0",
    "moment": "2.20.1",
    "pg": "7.4.1",
    "pg-pool": "2.0.3",
    "prop-types": "15.6.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-router-dom": "4.2.2",
    "react-tap-event-plugin": "3.0.2",
    "styled-components": "2.4.0",
    "superagent": "3.8.2"
  },
  "devDependencies": {
    "@types/baconjs": "0.7.33",
    "@types/big.js": "4.0.2",
    "@types/body-parser": "1.16.8",
    "@types/d3": "4.12.0",
    "@types/d3-axis": "1.0.9",
    "@types/express": "4.11.0",
    "@types/material-ui": "0.20.2",
    "@types/pg-pool": "0.0.3",
    "@types/react": "16.0.34",
    "@types/react-dom": "16.0.3",
    "@types/react-tap-event-plugin": "0.0.30",
    "@types/superagent": "3.5.6",
    "react-scripts-ts": "2.8.0"
  },
  "directories": {
    "test": "test"
  },
  "proxy": "http://localhost:3100",
  "description": "Kukkaro - a web tool for tracking your income and expenses."
}
